:document {
	
	head {
		if ($c.hasMaskBootstrap !== true) >
			script src="/.reference/atma_toolkit/node_modules/atma-utest/lib/env/vendor/mask.bootstrap.js";

		each ($c.scripts) >
			script src='~[.]';
	}
	body {
		
		
		script > :html > '''
			
			include
				.js(~[: $c.include ]);
			
			
			include
				.allDone(function(){
					var App = Compo({ compos: {} });

					window.app = new App;
					mask.Compo.bootstrap(document.body, app);
				});


		'''
	}
}