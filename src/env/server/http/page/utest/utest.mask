:document {
    body {
        
        style {
            '''
            body {
                background: #222;
                color: rgb(230,250,230);
                font-family: monospace;
            }
            '''
        }
    
        % each='scripts' {
            % if=path > script src='~[path]';
            % if=script > script > :html > '~[script]'
        }
        
    }
}
