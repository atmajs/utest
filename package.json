{
  "name": "atma-utest",
  "description": "TDD-Plugin for the Atma Toolkit",
  "author": {
    "name": "Alexander Kit",
    "email": "alex.kit@atmajs.com"
  },
  "version": "0.9.37",
  "devDependencies": {
    "atma": "latest",
    "atma-loader-babel": ">0.0.0"
  },
  "dependencies": {
    "assertion": "latest",
    "body-parser": "~1.4.3",
    "diff": "~1.0.8",
    "socket.io-client": "^1.3.5"
  },
  "atma": {
    "plugins": [
      "atma-loader-babel"
    ],
    "settings": {
      "atma-loader-babel": {
        "extensions": [
          "test",
          "es6"
        ]
      }
    }
  },
  "scripts": {
    "test": "atma test",
    "build": "atma"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/tenbits/atma-utest.git"
  },
  "license": "MIT"
}
