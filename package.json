{
    "name": "atma-utest",
    "description": "TDD-Plugin for the Atma Toolkit",
    "author": {
        "name": "Alexander Kit",
        "email": "alex.kit@atmajs.com"
    },
    "version": "0.9.15",
    "devDependencies": {
        "atma": "latest"
    },
    "dependencies": {
        "socket.io-client": ">1.0.0",
        "assertion": "latest",
        "body-parser": "~1.4.3",
        "diff": "~1.0.8",
        "atma-loader-traceur": ">0.0.0"
    },
    "atma": {
        "plugins": [
            "atma-loader-traceur"
        ],
        "settings": {
            "atma-loader-traceur": {
                "extensions": [
                    "test",
                    "es6"
                ]
            }
        }
    },
    "scripts": {
        "test" : "atma test",
        "build": "atma"
    },
    "repository": {
        "type": "git",
        "url": "git://github.com/tenbits/atma-utest.git"
    },
    "license": "MIT"
}